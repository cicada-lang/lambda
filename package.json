{
  "name": "@cicada-lang/lambda.js",
  "version": "0.3.1",
  "repository": "github:cicada-lang/lambda.js",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "main": "./lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "bin": {
    "lambda.js": "lib/main.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test:node": "node --test lib/**/*.test.js",
    "test:lang": "test-runner snapshot 'node ./lib/main.js run' 'examples/**/*.scm' --exclude 'examples/**/*.error.scm'",
    "test:lang:error": "test-runner snapshot-error 'node ./lib/main.js run' 'examples/**/*.error.scm'",
    "test": "npm run test:node && npm run test:lang && npm run test:lang:error",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@xieyuheng/commander.js": "^0.1.3",
    "dedent": "^1.6.0",
    "regexp-match-indices": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "@xieyuheng/test-runner": "^0.2.10",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.3"
  }
}
